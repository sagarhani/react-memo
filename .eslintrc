{
"parser": "babel-eslint",
    "extends": ["plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "strict": 0,
      "func-names": ["error", "never"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": "error"
    },
    "env": {
      "browser": true,
      "jest": true
    }
  }
  